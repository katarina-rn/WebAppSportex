<section id="products">
  <div class="container-fluid">
    <div class="row">
      <%categories.forEach(category =>{%>
      <div class="col-lg-3 col-md-6">
        <div class="card">
          <img class="card-img-top" src="/img/<%=category.url%>" alt="<%=category.title%>">
          <div class="card-body">
            <h5 class="card-title"><%=category.title%></h5>
            <p class="card-text"><%=category.description%></p>
            <form action="/proizvodi/<%=category.title%>" method="post" class="pages">
              <button type="submit" name="submit" class="seeMore"><span>Vidi vise</span></button>
            </form>
          </div>
        </div>
      </div>
      <%})%>
    </div>
  </div>
</section>
